import matplotlib.pyplot as plt

# Erstelle ein einfaches Vogelbild
fig, ax = plt.subplots()
ax.set_ylim(0, 10)
ax.set_xlim(0, 10)
ax.set_aspect('equal', adjustable='box')
ax.add_artist(plt.Circle((3, 6), 2, color='yellow'))
ax.add_artist(plt.Rectangle((2, 4), 4, 2, color='black'))
ax.add_artist(plt.Rectangle((4, 7), 1, 3, color='black'))
ax.add_artist(plt.Polygon([[3, 8], [4, 9], [5, 8]], color='black'))

plt.show()
